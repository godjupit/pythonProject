   "source": [
    "import numpy as np\n",
    "from scipy.optimize import linprog\n",
    "\n",
    "# 目标系数\n",
    "c = np.array([-72, -64])\n",
    "# 不等式约束矩阵\n",
    "A = [[1, 1], [12, 8]]\n",
    "# 右侧约束条件\n",
    "b = [50, 280]\n",
    "# 变量的范围\n",
    "bounds = ((0, 100/3.0), (0., None))\n",
    "\n",
    "# 使用 HiGHS 方法求解\n",
    "res = linprog(c, A_ub=A, b_ub=b, bounds=bounds)\n",
    "\n",
    "# 输出结果\n",
    "print(res)\n",
    "\n"
   ],
